  },
  "devDependencies": {
    "@stencil/angular-output-target": "0.10.2",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.21",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "puppeteer": "24.9.0",
